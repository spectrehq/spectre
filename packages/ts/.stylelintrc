{
  "plugins": ["stylelint-order", "stylelint-prettier"],
  "extends": ["stylelint-config-recommended"],
  "overrides": [],
  "rules": {
    "prettier/prettier": true,
    "no-descending-specificity": null,
    "max-nesting-depth": 2,
    "selector-max-id": 1,
    "order/properties-alphabetical-order": true
  }
}
